{
	"$schema": "https://biomejs.dev/schemas/1.9.3/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": false,
		"rules": {
			"style": {
				"useImportType": "off"
			}
		}
	},
	"organizeImports": {
		"enabled": true
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single",
			"lineWidth": 120
		}
	},
	"json": {
		"formatter": {
			"enabled": true
		}
	}
}
